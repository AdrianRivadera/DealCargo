<style>
    .social-buttons {
        display: flex;
        justify-content: center;
        align-items: center;
        background-color: #f2f2f2;
        box-shadow: 0px 0px 15px #00000027;
        padding: 10px 8px;
        border-radius: 5em;
    }

    .social-button {
        display: flex;
        justify-content: center;
        align-items: center;
        width: 2.4rem;
        height: 2.4rem;
        border-radius: 50%;
        margin: 0 10px;
        background-color: #fff;
        box-shadow: 0px 0px 4px #00000027;
        transition: 0.3s;
    }

    .social-button:hover {
        background-color: #AE1F23;
        box-shadow: 0px 0px 6px 3px #00000027;
    }

    .social-buttons svg {
        transition: 0.3s;
        height: 20px;
    }

    .facebook {
        background-color: #f2f2f2;
    }

    .facebook svg {
        fill: #AE1F23;
    }

    .facebook:hover svg {
        fill: #f2f2f2;
    }

    .github {
        background-color: #333;
    }

    .github svg {
        width: 25px;
        height: 25px;
        fill: #f2f2f2;
    }

    .github:hover svg {
        fill: #333;
    }

    .linkedin {
        background-color: #f2f2f2;
    }

    .linkedin svg {
        fill: #AE1F23;
    }

    .linkedin:hover svg {
        fill: #f2f2f2;
    }

    .instagram {
        background-color: #f2f2f2;
    }

    .instagram svg {
        fill: #AE1F23;
    }

    .instagram:hover svg {
        fill: #f2f2f2;
    }
</style>
